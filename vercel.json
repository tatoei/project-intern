{
    "version": 2,
    "builds": [
        {
            "src": "dist/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ],
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "startCommand": "npm run start:prod"
}